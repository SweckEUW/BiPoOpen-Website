{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext", 
    "moduleResolution": "Node",
    "strict": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "skipLibCheck": true,
    "baseUrl": ".",  
    "paths": {
      "@/*": ["src/*"],
    },                
  }
}